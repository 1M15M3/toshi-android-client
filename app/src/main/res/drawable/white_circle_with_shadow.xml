<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <solid
                android:color="#ccced4" />
            <corners
                android:radius="30dp" />
        </shape>

    </item>

    <item
        android:top="1px"
        android:bottom="3px"
        android:left="1px"
        android:right="2px">
        <shape xmlns:android="http://schemas.android.com/apk/res/android">
            <solid
                android:color="#ffffff" />
            <corners
                android:radius="30dp" />
        </shape>
    </item>

</layer-list>

